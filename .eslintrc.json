{
  "extends": ["next"],
  "plugins": ["simple-import-sort"],
  "rules": {
    "comma-dangle": ["warn", "always-multiline"],
    "eol-last": "error",
    "no-trailing-spaces": "error",
    "semi": ["warn", "never"],
    "simple-import-sort/imports": ["warn"],
    "simple-import-sort/exports": ["warn"]
  }
}
